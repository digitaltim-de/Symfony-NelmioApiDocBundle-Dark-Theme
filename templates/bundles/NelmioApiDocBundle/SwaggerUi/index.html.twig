<!DOCTYPE html>
<html>
<head>
    {% block meta %}
        <meta charset="UTF-8">
    {% endblock meta %}
    <title>{% block title %}{{ swagger_data.spec.info.title }}{% endblock title %}</title>

    {% block stylesheets %}
        {{ nelmioAsset(assets_mode, 'swagger-ui/swagger-ui.css') }}
        {{ nelmioAsset(assets_mode, 'style.css') }}
    {% endblock stylesheets %}

    {% block swagger_data %}
        {# json_encode(65) is for JSON_UNESCAPED_SLASHES|JSON_HEX_TAG to avoid JS XSS #}
        <script id="swagger-data" type="application/json">{{ swagger_data|json_encode(65)|raw }}</script>
    {% endblock swagger_data %}
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tailwindcss/2.2.19/tailwind.min.css"
          integrity="sha512-wnea99uKIC3TJF7v4eKk4Y+lMz2Mklv18+r4na2Gn1abDRPPOeef95xTzdwGD9e6zXJBteMIhZ1+68QC5byJZw=="
          crossorigin="anonymous" referrerpolicy="no-referrer"/>
    <script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap" rel="stylesheet">
    <style type="text/css">
        .swagger .btn,.swagger .swagger-container{box-shadow:var(--tw-ring-offset-shadow,0 0 #0000),var(--tw-ring-shadow,0 0 #0000),var(--tw-shadow)}.swagger .btn,.swagger .description p,.swagger .info .main span{color:rgb(255 255 255 / var(--tw-text-opacity));--tw-text-opacity:1}.swagger .btn{display:inline-flex;cursor:pointer;align-items:center;border-radius:.375rem;border-width:1px;border-color:transparent;--tw-bg-opacity:1;background-color:rgb(129 140 248 / var(--tw-bg-opacity));padding:.5rem .75rem;font-size:.875rem;line-height:1.25rem;font-weight:600;text-transform:uppercase;--tw-shadow:0 1px 2px 0 rgb(0 0 0 / 0.05);--tw-shadow-colored:0 1px 2px 0 var(--tw-shadow-color)}.swagger,.swagger .opblock-summary-path,.swagger .swagger-ui .dialog-ux .modal-ux-header h3,.swagger .title{--tw-text-opacity:1!important;color:rgb(255 255 255 / var(--tw-text-opacity))!important;font-family:Inter,sans-serif!important}.swagger .btn:hover{--tw-bg-opacity:1;background-color:rgb(99 102 241 / var(--tw-bg-opacity))}.swagger .btn:focus{outline:transparent solid 2px;outline-offset:2px;--tw-ring-offset-shadow:var(--tw-ring-inset) 0 0 0 var(--tw-ring-offset-width) var(--tw-ring-offset-color);--tw-ring-shadow:var(--tw-ring-inset) 0 0 0 calc(2px + var(--tw-ring-offset-width)) var(--tw-ring-color);box-shadow:var(--tw-ring-offset-shadow),var(--tw-ring-shadow),var(--tw-shadow,0 0 #0000);--tw-ring-opacity:1;--tw-ring-color:rgb(99 102 241 / var(--tw-ring-opacity));--tw-ring-offset-width:2px}.swagger{margin-top:2.5rem}.swagger .swagger-container{margin-top:2.5rem;border-radius:.5rem;background-color:rgb(255 255 255 / .1);padding:1.5rem;--tw-shadow:0 10px 15px -3px rgb(0 0 0 / 0.1),0 4px 6px -4px rgb(0 0 0 / 0.1);--tw-shadow-colored:0 10px 15px -3px var(--tw-shadow-color),0 4px 6px -4px var(--tw-shadow-color)}.swagger .scheme-container,.swagger .swagger-ui .opblock.opblock:hover{background-color:rgb(17 24 39 / .5)}.swagger .opblock-summary-path,.swagger .swagger-ui .dialog-ux .modal-ux-header h3,.swagger .title{font-size:1.5rem;line-height:2rem;font-weight:700}.swagger .description p,.swagger .scheme-container{line-height:1.75rem;border-radius:.5rem;padding:1rem}.swagger .description p{margin-top:1.5rem;--tw-bg-opacity:1;background-color:rgb(17 24 39 / var(--tw-bg-opacity));font-size:1.125rem}.swagger .info .main span{margin-left:1rem;display:inline-flex;flex-direction:row;align-items:center;justify-content:center;gap:1rem;border-radius:.5rem;padding:1rem}.swagger .scheme-container{margin-left:1.25rem;margin-right:1.25rem;font-family:Inter,sans-serif!important;font-size:1.25rem;--tw-text-opacity:1!important;color:rgb(255 255 255 / var(--tw-text-opacity))!important}.swagger .opblock-title,.swagger .servers-title,.swagger .swagger-ui .opblock .opblock-section-header{font-size:1.5rem;line-height:2rem;font-weight:700;--tw-text-opacity:1!important;color:rgb(255 255 255 / var(--tw-text-opacity))!important}.swagger .schemes{padding:0}.swagger .swagger-ui{width:100%}.swagger .servers-title{display:none;padding-bottom:1rem;font-family:Inter,sans-serif!important}.swagger .servers{margin-bottom:1rem;width:100%;border-radius:.5rem;padding:0;font-family:Inter,sans-serif}.swagger .servers::before{display:block;height:2.5rem;--tw-content:'Api-Endpoint';content:var(--tw-content)}.swagger .swagger-ui .servers>label select{width:100%;cursor:pointer;border-radius:.5rem;--tw-bg-opacity:1;background-color:rgb(255 255 255 / var(--tw-bg-opacity));padding:1rem;--tw-text-opacity:1;color:rgb(17 24 39 / var(--tw-text-opacity))}.swagger .opblock-title,.swagger .swagger-ui .opblock .opblock-section-header{--tw-bg-opacity:1;background-color:rgb(17 24 39 / var(--tw-bg-opacity));font-family:Inter,sans-serif!important}.swagger .swagger-ui .opblock-description-wrapper p,.swagger-ui .opblock-external-docs-wrapper p,.swagger-ui .opblock-title_normal p{--tw-text-opacity:1!important;color:rgb(255 255 255 / var(--tw-text-opacity))!important}.swagger .swagger-ui .opblock .opblock-section-header h4,.swagger .swagger-ui .response-col_status,.swagger .swagger-ui table thead tr td,.swagger-ui table thead tr th{color:rgb(255 255 255 / .8)!important}.swagger .swagger-ui .btn.authorize:hover{--tw-bg-opacity:1;background-color:rgb(31 41 55 / var(--tw-bg-opacity))}.swagger .swagger-ui .dialog-ux .modal-ux,.swagger-ui button,.swagger-ui input{border-radius:.5rem;--tw-bg-opacity:1;background-color:rgb(31 41 55 / var(--tw-bg-opacity));padding:1rem;--tw-text-opacity:1;color:rgb(255 255 255 / var(--tw-text-opacity))}.swagger-ui button:hover,.swagger-ui input:hover{--tw-bg-opacity:1;background-color:rgb(17 24 39 / var(--tw-bg-opacity))}
    </style>
</head>
<body class="bg-gray-900 w-full justify-center flex mt-6 mb-12">
<div class="swagger max-w-7xl w-full">
    {% block swagger_ui %}
        <div class="flex flex-row">
            <a id="logo" href="https://github.com/nelmio/NelmioApiDocBundle">
                <img src="{{ nelmioAsset(assets_mode, 'logo.png') }}" alt="NelmioApiDocBundle">
            </a>
        </div>
        <div id="swagger-ui" class=""></div>
    {% endblock %}

    {% block javascripts %}
        {{ nelmioAsset(assets_mode, 'swagger-ui/swagger-ui-bundle.js') }}
        {{ nelmioAsset(assets_mode, 'swagger-ui/swagger-ui-standalone-preset.js') }}
    {% endblock javascripts %}

    {{ nelmioAsset(assets_mode, 'init-swagger-ui.js') }}

    {% block swagger_initialization %}
        <script type="text/javascript">
            (function () {
                var swaggerUI = {{ swagger_ui_config|json_encode(65)|raw }};
                window.onload = loadSwaggerUI(swaggerUI);
            })();
        </script>
    {% endblock swagger_initialization %}
</div>
</body>
</html>
